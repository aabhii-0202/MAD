<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundcolor"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:paddingTop="16dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="16dp"
        android:padding="8dp"
        android:layout_marginVertical="16dp"
        android:background="@drawable/indexlist_background"
        android:orientation="horizontal">

        <androidx.cardview.widget.CardView
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:cardCornerRadius="40dp">
        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/rnslogo"
            android:scaleType="centerCrop"
            />
        </androidx.cardview.widget.CardView>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginHorizontal="16dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Abhishek Kumar Singh"
                android:textColor="@color/black"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="1RN19IS005"
                android:textColor="@color/black"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginHorizontal="16dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ISE 'A'"
                android:textColor="@color/black"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="6th Sem"
                android:textColor="@color/black"/>
        </LinearLayout>




    </LinearLayout>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Mobile Application Development Lab "
        android:textStyle="bold"
        android:textColor="@color/indexText"
        android:background="@drawable/indexlist_background"
        android:padding="16dp"
        android:textSize="16sp"
        android:textAlignment="center"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="16dp"
        />

    <TextView
        android:id="@+id/one"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="1.Visiting Card"
        android:textStyle="bold"
        android:textColor="@color/indexText"
        android:background="@drawable/indexlist_background"
        android:padding="8dp"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="16dp"
        />

    <TextView
        android:id="@+id/two"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="2.Simple Calculator"
        android:textStyle="bold"
        android:textColor="@color/indexText"
        android:background="@drawable/indexlist_background"
        android:padding="8dp"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="16dp"
        />

    <TextView
        android:id="@+id/three"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="3.Login Screen"
        android:textStyle="bold"
        android:textColor="@color/indexText"
        android:background="@drawable/indexlist_background"
        android:padding="8dp"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="16dp"
        />

    <TextView
        android:id="@+id/four"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="4.Wallpaper"
        android:textStyle="bold"
        android:textColor="@color/indexText"
        android:background="@drawable/indexlist_background"
        android:padding="8dp"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="16dp"
        />

    <TextView
        android:id="@+id/five"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="5.Counter"
        android:textStyle="bold"
        android:textColor="@color/indexText"
        android:background="@drawable/indexlist_background"
        android:padding="8dp"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="16dp"
        />

    <TextView
        android:id="@+id/six"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="6.XML JSON"
        android:textStyle="bold"
        android:textColor="@color/indexText"
        android:background="@drawable/indexlist_background"
        android:padding="8dp"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="16dp"
        />

    <TextView
        android:id="@+id/seven"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="7.Text to Speech"
        android:textStyle="bold"
        android:textColor="@color/indexText"
        android:background="@drawable/indexlist_background"
        android:padding="8dp"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="16dp"
        />

    <TextView
        android:id="@+id/eight"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="8.Phone Dialer"
        android:textStyle="bold"
        android:textColor="@color/indexText"
        android:background="@drawable/indexlist_background"
        android:padding="8dp"
        android:layout_marginVertical="8dp"
        android:layout_marginHorizontal="16dp"
        />



</LinearLayout>